{
  "name": "pure-thin-cli",
  "version": "0.1.0",
  "description": "vue-pure-admin脚手架",
  "main": "bin/www.js",
  "scripts": {
    "dev": "pnpm build && pure init thin demo",
    "build": "tsup",
    "typecheck": "tsc --noEmit",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,ts,md}\"",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "lint": "pnpm lint:eslint && pnpm lint:prettier",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "keywords": [
    "pure-thin-cli",
    "pure-admin-thin",
    "vue-pure-admin"
  ],
  "author": {
    "name": "Ten-K",
    "email": "742798240@qq.com"
  },
  "license": "MIT",
  "bin": {
    "pure": "bin/www.js"
  },
  "files": [
    "dist",
    "bin"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ten-K/pure-thin-cli.git"
  },
  "homepage": "https://github.com/Ten-K/pure-thin-cli#readme",
  "dependencies": {
    "cac": "^6.7.14",
    "figlet": "^1.5.2",
    "fs-extra": "^10.1.0",
    "inquirer": "^8.2.4",
    "log-symbols": "^4.1.0",
    "ora": "^5.4.1",
    "picocolors": "^1.0.0",
    "simple-git": "^3.14.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.1.2",
    "@commitlint/config-conventional": "^17.1.0",
    "@types/figlet": "^1.5.5",
    "@types/fs-extra": "^9.0.13",
    "@types/inquirer": "^8.2.4",
    "@types/node": "^18.11.3",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "eslint": "^8.25.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.6.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "2.7.1",
    "pretty-quick": "^3.1.3",
    "tsup": "^6.3.0",
    "typescript": "^4.8.4"
  }
}
